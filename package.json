{
  "name": "express-ts-server",
  "version": "1.0.0",
  "description": "An Express server written in TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "start:local:db": "bash ./scripts/startLocalDB.sh",
    "lint": "npx eslint 'src/**/*.{ts,js,tsx}'",
    "lint:fix": "npx eslint 'src/**/*.{ts,js,tsx}' --fix",
    "prettier": "npx prettier 'src/**/*.{ts,js,tsx}' --write",
    "prettier:check": "npx prettier 'src/**/*.{ts,js,tsx}' --check",
    "migrate:make": "npx knex migrate:make $1 --knexfile src/db/knexfile.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "knex": "^3.1.0",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.2",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.19.1"
  }
}
