{
  "name": "express-ts-server",
  "version": "1.0.0",
  "description": "An Express server written in TypeScript",
  "main": "./src/index.js",
  "_moduleAliases": {
    "@/root": "./",
    "@": "./src"
  },
  "exports": {
    "import": "./src/index.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "source ./scripts/unpackSecrets.sh && source ./scripts/setDbEnvs.sh && node -r tsconfig-paths/register src/index.js",
    "unpackSecrets": "source ./scripts/unpackSecrets.sh",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "test": "jest",
    "dev": "nodemon --inspect=127.0.0.1:9231 --verbose --config nodemon.json",
    "start:local:db": "bash ./scripts/startLocalDB.sh",
    "lint": "npx eslint 'src/**/*.{ts,js,tsx}'",
    "lint:fix": "npx eslint 'src/**/*.{ts,js,tsx}' --fix",
    "prettier": "npx prettier 'src/**/*.{ts,js,tsx}' --write",
    "prettier:check": "npx prettier 'src/**/*.{ts,js,tsx}' --check"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.4.2",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "esbuild-jest": "^0.5.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.19.1"
  }
}
